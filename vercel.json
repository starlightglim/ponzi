{
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html",
        "headers": {
          "Cache-Control": "no-cache, no-store, must-revalidate"
        }
      },
      {
        "src": "/(.*\\.js)",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/(.*\\.css)",
        "headers": {
          "Content-Type": "text/css"
        }
      },
      {
        "src": "/(.*\\.(png|jpg|jpeg|gif|svg))",
        "headers": {
          "Content-Type": "image/$1"
        }
      }
    ]
  }